          <option>9</option>
          <option>10</option>
          <option>10+</option>
          <option>20+</option>
          <option>30+</option>
        </select>
      </div>

<!-- ///////should show items in a drop down by using table info///// -->
<div class="form-group"action="/current" method="POST">
  Items: 
  <select name="" id="cat">
    <% Items.forEach(record => { %>
    <option value="<%= record.id%>"> <%= record.Food%> </option>
    <% }) %>
  </select>
</div>






